<div>
  <div class="row">
    <div class="col-md-10"><h1>Projects</h1></div>
  </div>
  <ul class="list-group list-group-flush">
    <% for(var i=0; i < data.length; i++){ %>
    <li class="list-group-item">
      <%- include('project.ejs',{title: data[i].title,
      description:data[i].description, path: data[i].path, role: data[i].role})
      %>
    </li>
    <% } %>
  </ul>
</div>
